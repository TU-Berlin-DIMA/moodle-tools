-- Foreign key columns
SELECT lower(unnest(constraint_column_names)) AS name_fk_from, lower(constraint_text) as fk_constraint_text
from duckdb_constraints()
where
    schema_name='main'
  and constraint_type = 'FOREIGN KEY'
  and lower(table_name)=lower('{{ tablename }}')
ORDER BY lower(name_fk_from);
